prometheus.scrape "test_address" {
  scrape_interval = "42s"
  targets = [{ __address__ = "localhost:12345", }]
  forward_to = [
    prometheus.relabel.test_address.receiver,
  ]
}
prometheus.relabel "test_address" {
  forward_to = [
    prometheus.remote_write.another.receiver,
    prometheus.remote_write.basic_empty.receiver,
    prometheus.remote_write.basic_nil.receiver,
    prometheus.remote_write.basic_no_password.receiver,
    prometheus.remote_write.basic_no_username.receiver,
    prometheus.remote_write.grafana_cloud.receiver,
    prometheus.remote_write.grafana_cloud_interval.receiver,
  ]
}
prometheus.scrape "test_relabel" {
  scrape_interval = "42s"
  targets = [{ __address__ = "localhost:12345", }]
  forward_to = [
    prometheus.relabel.test_relabel.receiver,
  ]
}
prometheus.relabel "test_relabel" {
  forward_to = [
    prometheus.remote_write.another.receiver,
    prometheus.remote_write.basic_empty.receiver,
    prometheus.remote_write.basic_nil.receiver,
    prometheus.remote_write.basic_no_password.receiver,
    prometheus.remote_write.basic_no_username.receiver,
    prometheus.remote_write.grafana_cloud.receiver,
    prometheus.remote_write.grafana_cloud_interval.receiver,
  ]
  rule {
    source_labels = [
      "l1",
      "l2",
    ]
    separator = ";"
    target_label = "t1"
    replacement = "$1"
    regex = "r2"
    action = "drop"
  }
  rule {
    source_labels = [
      "l1",
    ]
    action = "drop"
  }
}
