logging {
    level = "info"
    format = "logfmt"
    write_to = [
      loki.write.another.receiver,
      loki.write.basic_empty.receiver,
      loki.write.basic_nil.receiver,
      loki.write.basic_no_password.receiver,
      loki.write.basic_no_username.receiver,
      loki.write.grafana_cloud.receiver,
    ]
}
prometheus.remote_write "another" {
  endpoint {
    url = "https://another.com/prom/push"
    bearer_token = "secret"
    tls_config {
      insecure_skip_verify = true
    }
  }
  external_labels = {
  }
}
